import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int i=0; i<T; i++) {
			int N = sc.nextInt();
			int sum = 0;
			
			for(int j=0; j<=N; j++) {
				if(j%2 == 1) {
					sum += j;
				}
				
			}
			
			System.out.println(sum);
		}
		
		sc.close();
	}

}
