import java.io.IOException;
import java.util.Scanner;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int i=0; i<T; i++) {
			int sum = 0;
			int N = sc.nextInt();
			for(int j=0; j<N; j++) {
				int a = sc.nextInt();
				sum += a;
			}
			System.out.println(sum);
		}
		
		
		
	}

}
