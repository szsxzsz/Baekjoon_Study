import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		
		double M = (double) (B-A)/400;
		
		 System.out.printf("%.6f", 1 / (1 + Math.pow(10, M)));
		
		sc.close();
	}

}
