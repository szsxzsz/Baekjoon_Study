import java.io.IOException;
import java.util.Scanner;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int a,b,c,d;
		
		while(true) {
			a = sc.nextInt();
			b = sc.nextInt();
			c = sc.nextInt();
			d = sc.nextInt();
			
			if(a == 0 & b == 0 & c == 0 & d == 0) {
				break;
			}
			
			System.out.println((c-b)+" "+(d-a));
			
		}
		
		sc.close();
		
	}

}
