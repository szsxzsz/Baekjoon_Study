import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		String plus = sc.next();
		int b = sc.nextInt();
		String equal = sc.next();
		int c = sc.nextInt();
		
		if(a+b == c) {
			System.out.println("YES");
		} else {
			System.out.println("NO");
		}
		
		sc.close();
	}

}
