import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt(); // 8
		int b = sc.nextInt(); // 3
		
		System.out.println(8*a+3*b-28);
		
		sc.close();
	}

}
