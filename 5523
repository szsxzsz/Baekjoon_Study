import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		int A = 0, B = 0;
		
		for(int i = 0; i < T; i++) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			
			if(a > b) {
				A++;
			} else if (b > a){
				B++;
			} else {
				A = A;
				B = B;
			}
		}
		
		System.out.println(A+" "+B);
		
		sc.close();
	}

}
