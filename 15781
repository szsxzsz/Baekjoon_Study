import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		int aMax = 0, bMax = 0;
		
		for(int i=0; i<a; i++) {
			int num = sc.nextInt();
			
			if(num > aMax) {
				aMax = num;
			}
		}
		
		for(int i=0; i<b; i++) {
			int num = sc.nextInt();
			
			if(num > bMax) {
				bMax = num;
			}
		}
		
		System.out.println(aMax+bMax);
		
		sc.close();
	}

}
