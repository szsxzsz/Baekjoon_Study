import java.math.BigInteger;
import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int i=0; i<T; i++) {
			BigInteger num = sc.nextBigInteger();
			
			System.out.println(num.remainder(BigInteger.valueOf(2)).equals(BigInteger.ZERO) ? "even" : "odd");	
		}
		
		sc.close();
	}

}
