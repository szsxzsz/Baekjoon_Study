import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		String a = sc.next();
		int num = 0;
		
		for(int i=0; i<a.length(); i++) {
			char c = a.charAt(i);
			
			if(c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u') {
				num++;
			}
		}
		
		System.out.println(num);
		sc.close();
	}

}
