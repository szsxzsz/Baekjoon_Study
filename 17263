import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		int a;
		int max = 0;
		
		for(int i=0; i<T; i++) {
			a = sc.nextInt();
			
			if(a>=max) {
				max = a;
			}
		}
		
		System.out.println(max);
		
		sc.close();
	}

}
