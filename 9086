import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		for(int i=0; i<n; i++) {
			String s = sc.next();
			int j = s.length();
			
			String first = String.valueOf(s.charAt(0));
			String last = String.valueOf(s.charAt(j-1));
			System.out.println(first+last);
		}
		
		
		
		
	}

}
