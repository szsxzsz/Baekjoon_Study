import java.io.IOException;
import java.util.Scanner;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
	
		int T = sc.nextInt();
		
		int sin = 0;
		
		for(int i=0; i<T; i++) {
			int n = sc.nextInt();
			if(n != i+1) {
				sin++;
			}
		}
		System.out.println(sin);
		
		sc.close();
		
	}

}
