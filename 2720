import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int Q = 25;
		int D = 10;
		int N = 5;
		int P = 1;
		
		StringBuilder sb = new StringBuilder();
		
		int T = sc.nextInt();
		
		for(int i=0;i<T;i++) {
			int C = sc.nextInt();
			sb.append(C/Q+" ");
			C = C%Q;
			sb.append(C/D+" ");
			C = C%D;
			sb.append(C/N+" ");
			C = C%N;
			sb.append(C/P+"\n");
		}
		System.out.println(sb);
		
	}

}
