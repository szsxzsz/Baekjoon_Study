import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		String S = sc.next();
		int num[] = new int[26];
		
		for(int i=0; i<S.length(); i++) {
			num[S.charAt(i)-97]++;
		}
		for(int i=0; i<26; i++) {
			System.out.print(num[i]+" ");
		}
		
		sc.close();
	}

}
