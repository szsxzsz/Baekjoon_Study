import java.math.BigInteger;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		BigInteger A;
		BigInteger B;
		
		for(int i = 0; i < a; i++) {
			A = sc.nextBigInteger();
			B = sc.nextBigInteger();
			System.out.println(A.add(B));
		}

	}

}
