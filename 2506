import java.io.*;
import java.util.*;


public class Main {

	public static void main(String[] args) throws NumberFormatException, IOException {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		int sum = 0, score = 0;
		
		for(int i=0; i<T; i++) {
			int stage = sc.nextInt();
			
			if(stage == 1) {
				score ++;
				sum += score;
			} else {
				score = 0;
			}
		}
		
		System.out.println(sum);
		
		sc.close();
	}

}
