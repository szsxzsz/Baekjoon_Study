import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		
		Scanner scan = new Scanner(System.in);
	
		int N = scan.nextInt();
		
		for(int j=1; j<=N; j++) {

			for(int i=1; i<=N-j; i++) {
				System.out.print(" ");
			}	
			for(int k=1;k<=j;k++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
		
		
		scan.close();
		

	}

}
