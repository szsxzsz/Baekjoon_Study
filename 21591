import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a1 = sc.nextInt();
		int b1 = sc.nextInt();
		int a2 = sc.nextInt();
		int b2 = sc.nextInt();
		
		if(a1 > a2+1 && b1 > b2+1) {
			System.out.println(1);
		} else {
			System.out.println(0);
		}
		
	}

}
